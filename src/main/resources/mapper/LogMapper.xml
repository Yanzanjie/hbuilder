<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.yzj.springboot.hbuilder.mapper.LogMapper">

    <select id="getUId" resultType="int" parameterType="String">
        select id from log where username = #{name}
    </select>

</mapper>
